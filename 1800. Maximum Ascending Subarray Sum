class Solution {
public:
    int maxAscendingSum(vector<int>& nums) {
        int s = nums.size();
        int sum = nums[0];
        int ans = nums[0];
        for(int i=1; i<s; i++){
            if(nums[i-1]< nums[i]) sum+= nums[i];
            else{
                ans = max(ans, sum);
                sum = nums[i];
            }
        }

        ans = max(ans, sum);

        return ans;
    }
};
